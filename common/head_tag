<script type="text/discourse-plugin" version="0.8.23">
  api.addSharingSource({
    id: "$social_id",
    icon: "$social_icon",
    generateUrl: function(link, title) {
      return "&social_link" + encodeURIComponent(link)  + "&title=" + 
encodeURIComponent(title);
    },
    shouldOpenInPopup: true,
    popupHeight: 265
  })
</script>
